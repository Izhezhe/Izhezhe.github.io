<table cellpadding="0" cellspacing="0">
       		<tr>
       			<th>参评时间</th>
       			<th>案件名称</th>
       			<th>案号</th>
       			<th>审理法院</th>
       			<th>状态</th>
       			<th>操作</th>
       		</tr>
            {{~it :value:index}}
       		<tr>
       			<td>{{?value.releaseTime}}{{=format(value.releaseTime)}}{{?}}</td>
       			<td class="case-name" style="width:240px;"><div class="ell" style="width:240px;" title="{{=value.wsCase.title}}">
                        {{?value.wsCase.title}}{{=value.wsCase.title}}{{?}}</div></td>
       			<td style="width:102;"><div style="width:102px;" class="ell">{{?value.wsCase.caseNo}}{{=value.wsCase.caseNo}}{{?}}</div></td>
       			<td style="width:91px;"><div style="width:91px;" class="ell">{{?value.wsCase.courtName}}{{=value.wsCase.courtName}}{{?}}</div></td>
                         {{? value.wsCase.liveState===0 }}
                         <td>案件预告</td>
                         {{?? value.wsCase.liveState===1}}
                         <td>直播中</td>
                         {{?? value.wsCase.liveState===3}}
                         <td>案件回顾</td>
                         {{?}}

       			<td class="control">
                              <span class="public-button2"><a href="./caseSet?id={{=value.id}}">设置</a></span>
                        </td>
       		</tr>
            {{~}}
       	</table>