                  <table cellpadding="0" cellspacing="0">
                        <tr>
                              <th>姓名</th>
                              <th>职务描述</th>
                              <th>评分状态</th>
                              <th>分数</th>
                              <th>操作</th>
                        </tr>
                  {{~it :value:index}}
                        <tr>
                              <td class="name">{{?value.name}}{{=value.name}}{{?}}</td>
                              <td class="post"><div class="ell">{{?value.jobDesc}}{{=value.jobDesc}}{{?}}</div></td>
                              {{? value.totalScore > 0}}
                              <td class="">已评分</td>
                              {{??}}
                              <td class="">未评分</td>
                              {{?}} 
                              <td class="stroe">{{?value.totalScore}}{{=value.totalScore}}{{?}}</td>
                              <td class="null">
                                    <span style="width:84px;position:relative;" class="past scoreReasonT" temp =
                                    {{?value.scoreReason}}
                                          {{=value.scoreReason}}
                                    {{??}}
                                          未填写
                                    {{?}} >
                                       {{?value.isComment == 1}}
                                          <a href="./detailsShow?caseId={{=value.caseId}}" style="width: 84px;display: inline-block;">查看打分理由</a>
                                       {{??}}
                                          查看打分理由
                                       {{?}} 
                                    </span>
                              </td>
                        </tr>
                  {{~}}
                  </table>