            <table cellpadding="0" cellspacing="0">
                  <tr>
                        <th>参评时间</th>
                        <th>案件名称</th>
                        <th>案号</th>
                        <th>指派状态</th>
                        <th>打分状态</th>
                        <th>点评状态</th>
                        <th>操作</th>
                  </tr>
                {{~it : value:index}}
                  <tr>
                        <td>{{?value.releaseTime}}{{=formatDate(value.releaseTime)}}{{?}}</td>
                        <td class="case-name">
                              <div class="ell" title="{{=value.wsCase.title}}">{{=value.wsCase.title}}</div>
                        </td>
                        <td class="case-number"><div class="ell">{{=value.wsCase.caseNo}}</div></td>
                       
                        <td>
                               {{? value.caseAppointList.length>0}}
                                <span>
                                    {{~value.caseAppointList:caseAppoint:index1}}
                                      {{? caseAppoint.name}}
                                       已指派-{{=caseAppoint.name}}<br/>
                                       {{??}}
                                       未指派<br/>
                                       {{?}}                                    
                                    {{~}}
                                </span>
                                {{??}}
                                 <span>
                                   未指派<br/> 
                                 </span>
                                {{?}}                                                           
                        </td>
                        <td>
                              {{? value.caseAppointList.length>0}}
                              <span>
                                    {{~value.caseAppointList:caseAppoint:index2}}
                                          {{? caseAppoint.totalScore > 0}}
                                          已打分<br/>
                                          {{??}}
                                          未打分<br/>
                                          {{?}}                                   
                                    {{~}}
                              </span>

                             {{??}}
                               <span>
                                 未打分<br/> 
                               </span> 
                            {{?}}       
                         </td>
                         <td>
                          {{? value.caseAppointList.length>0}}
                               <span>
                              {{~value.caseAppointList:caseAppoint:index3}}
                              {{? caseAppoint.isComment===1 }}
                              已点评<br/>                                       
                              {{??}}
                              未点评<br/>
                              {{?}} 
                              {{~}}
                              </span>
                             {{??}}
                               <span>
                                 未点评<br/> 
                               </span>
                             {{?}}         
                         </td>
                        <td class="control">
                              {{? value.appointFlag === 1}}
                              <a href="./assignJudges?caseId={{=value.caseId}}">编辑指派</a>
                              {{?}}
                              <a href="./viewComment?id={{=value.caseId}}">查看</a>
                        </td>
                  </tr>
                {{~}}
            </table>
