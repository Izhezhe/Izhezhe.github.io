       	<table cellpadding="0" cellspacing="0">
       		<tr>
       			<th>编号</th>
       			<th>姓名</th>
       			<th>职务描述</th>
       			<th>录入时间</th>
       			<th>操作</th>
       		</tr>

            {{~it :value:index}}
                  <tr id={{=value.id}}>
                        <td>{{=index+1}}</td>
                        <td>{{?value.name}}{{=value.name}}{{?}}</td>
                        <td class="post"><div class="ell">{{?value.jobDesc}}{{=value.jobDesc}}{{?}}</div></td>
                        <td>{{?value.createTime}}{{=format(value.createTime)}}{{?}}</td>
                        <td class="check"><span class=""></span>指派</td>
                  </tr>
            {{~}}
       	</table>