<table cellpadding="0" cellspacing="0">
       		<tr>
       			<th>编号</th>
       			<th>小组名称</th>
       			<th>组长</th>
       			<th>组员</th>
       			<th>操作</th>
       		</tr>
            {{~it :value:index}}
       		<tr>
                        <td class="number">{{=index+1}}</td>
                        <td class="name">{{=value.name}}</td>
       			<td class="job">{{?value.leaderuserName}}{{=value.leaderuserName}}{{??}}无组长{{?}}</td>
       			<td>{{=value.groupcount}}人</td>
       			<td class="control">
                              <span class="public-button2"><a href="./addEditGroup?id={{=value.id}}" style="display: block;">编辑组员</a></span>
                        </td>
       		</tr>
            {{~}}
       		
       	</table> 