<table cellpadding="0" cellspacing="0">
	<tr>
		<th>编号</th>
		<th>姓名</th>
		<th>职务描述</th>
		<th>所属小组</th>
		<th>操作</th>
	</tr>
    {{~it :value:index}}
	<tr>
	    <td class="number ell">{{= index+1}}</td>
		<td class="name ell">{{=value.name}}({{=value.loginname}})</td>
		<td class="job ell"><div class="ell">{{=value.jobDesc}}</div></td>
		{{? value.wasctivityUserGroup }}
          	<td>{{=value.wasctivityUserGroup.name}}</td>
        {{??}}
        	<td>无分组</td>
        {{?}}
		<td class="control">
          <a href="./addJudges?id={{=value.id}}" class="public-button2">编辑</a>
        </td>

	</tr>
    {{~}}
</table>