<table cellpadding="0" cellspacing="0">
	<tr>
		<th>编号</th>
		<th>姓名</th>
		<th>职务描述</th>
		<th>录入时间</th>
		<th>操作</th>
	</tr>
  {{~it :value:index}}
    <tr>
      <td>{{=index+1}}</td>
      <td class="name">{{=value.name}}</td>
      <td class="post"><div class="ell">{{=value.jobDesc}}</div></td>
      <td>{{?value.createTime}}{{=format(value.createTime)}}{{?}}</td>
      <td class="control" id="{{=value.id}}">
        <span>
          {{? value.isSelected == 0}}
            <em class=></em>
          {{?? value.isSelected == 1}}
            <em class="on"></em>
          {{?}}
          加入小组
        </span>
        <span class="selectLeader"><em></em>设置组长</span>
      </td>
    </tr>   
  {{~}}       
</table>