<table cellpadding="0" cellspacing="0">
      <tr>
            <th>案件名称</th>
            <th>案号</th>
            <th>位置</th>
            <th>操作</th>
      </tr>
      {{~it.data:value:index}}
      <tr>
           <td class="case-name">
              <div class="ell">{{=value.wsCase.title}}</div>
            </td>
            <td class="case-number"><div class="ell">{{=value.wsCase.caseNo}}</div></td>
            {{? value.seq==1}}
            <td class="local">位置一</td>  
            {{?}}
            {{? value.seq==2  }}
            <td class="local">位置二</td>
            {{?}}
            {{? value.seq==3  }}
            <td class="local">位置三</td>
            {{?}}
            {{? value.seq==4  }}
            <td class="local">位置四</td>
            {{?}}
            {{? value.seq==5  }}
            <td class="local">位置五</td>
            {{?}}
            
            {{? index==0}}
                  <td class="control" rowspan="5">
                        <span id="recommend-set-edit"  releaseTime="{{=format(value.releaseTime)}}">编辑</span>
                        <span id="recommend-set-delete" releaseTime="{{=format(value.releaseTime)}}">删除</span>
                  </td>
            {{?}}
      </tr>
      {{~}}
</table>