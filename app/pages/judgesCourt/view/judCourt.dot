<table cellpadding="0" cellspacing="0">
	<tr>
		<th>法院名称</th>
		<th>所属省份</th>
		<th>参评案件数</th>
	</tr>
    {{~it :value:index }}
	<tr>
		<td style="width:350px">
			<div class="ell" style="width:350px">{{=value.courtName}}</div>
		</td>
		<td  style="width:300px" class="ell">
			 <div class="ell" style="width:300px">{{=value.provinceName}}</div>
		</td>
		<td class="ell">{{=value.caseCount}}</td>
	</tr>
    {{~}}
</table>